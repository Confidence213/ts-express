{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "linebreak-style": [
      "error",
      "windows"
    ],
    "import/no-unresolved": [
      0,
      {
        "caseSensitive": false
      }
    ],
    "import/extensions": [
      2,
      "never",
      {
        "route": "always",
        "model": "always"
      }
    ],
    "consistent-return": ["off"],
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
    "max-len": ["error", { "code": 120 }],
    "no-unused-vars": "off",
    "no-underscore-dangle": "off",
    "@typescript-eslint/no-unused-vars": ["error"]
  }
}